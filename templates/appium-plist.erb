<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
  <dict>
    <key>KeepAlive</key>
    <dict>
      <key>SuccessfulExit</key>
      <false/>
    </dict>
    <key>Label</key>
    <string><%=@plist_label%></string>
    <key>ProgramArguments</key>
    <array>
      <string>/usr/local/bin/node</string>
      <string><%=@appium%></string>
      <string>--session-override</string>
      <string>--log-no-colors</string>
      <string>--log-level</string>
      <string>warn</string>
      <string>--nodeconfig</string>
      <string><%=@node_config%></string>
    </array>
    <key>Debug</key>
    <true/>
    <key>RunAtLoad</key>
    <true/>
    <key>StandardOutPath</key>
    <string>/Users/appium/Library/Logs/appium-<%=@name%>.log</string>
    <key>StandardErrorPath</key>
    <string>/Users/appium/Library/Logs/appium-<%=@name%>_error.log</string>
  </dict>
</plist>