#! /bin/sh
# This defaults file is managed by the selenium puppet module.
# Editing is futile !

export ANDROID_HOME=<%=@android_home%>

exec <%=@appium%> --port <%=@port%> --chromedriver-port <%=@chromedriver_port%> --bootstrap-port <%=@bootstrap_port%> --nodeconfig <%=@node_config%> --log-no-colors --log-level warn