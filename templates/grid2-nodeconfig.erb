{
  "capabilities": <%=@capabilities.to_json%>,
  "configuration": {
    "cleanUpCycle":2000,
    "timeout":30000,
    "proxy": "org.openqa.grid.selenium.proxy.DefaultRemoteProxy",
    "host": "<%=@host%>",
    "url": "http://<%=@host%>:<%=@port%>/wd/hub",
    "port": "<%=@port%>",
    "maxSession": <%=@max_session%>,
    "register": true,
    "registerCycle": 5000,
    "hubPort": "<%=@hub_port%>",
    "hubHost": "<%=@hub_host%>"
  }
}